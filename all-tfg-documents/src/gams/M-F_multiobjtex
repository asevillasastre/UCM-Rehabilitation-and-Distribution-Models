\begin{document}
\section*{Symbols}
\subsection*{Sets}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
j, i & * & Nodos. \\ 
v & * & Tipos de convoy. \\ 
\hline 
\end{tabularx}
\subsection*{Parameters}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
dem & j & Demanda de cada nodo en toneladas de paquetes de ayuda. \\ 
capacidad & v & Capacidad en toneladas de una unidad vehicular de tipo v. \\ 
coste\_fijo & v & Coste fijo en dolares de hacer partir un convoy de tipo v. \\ 
coste\_variable & v & Coste variable en USD por km recorrido de cada convoy. \\ 
M &  & Cota de flujo maximo en toneladas. \\ 
max\_coste\_rehab &  & Maximo presupuesto que se puede invertir en total en la mision en reconstruccion en millones de USD. \\ 
max\_coste &  & Maximo presupuesto que se puede invertir en total en la mision en transporte en USD. \\ 
max\_ayuda &  & Toneladas de ayuda de las que se dispone. \\ 
PS\_total &  & Probabilidad aceptable de que todos los convoys puedan pasar por todos los arcos elegidos por motivos de seguridad. \\ 
P\_total &  & Probabilidad aceptable de que todos los convoys puedan pasar por todos los arcos elegidos por motivos logisticos. \\ 
E & i, j & Existe el arco de i a j. \\ 
U & i, j & Al inicio es utilizable el arco de i a j. \\ 
dist & i, j & Distancia de los arcos de i a j en km. \\ 
coste\_rehabilitacion & i, j & Coste en M de USD de rehabilitar el arco de i a j. \\ 
P & i, j & Probabilidad de que se pueda utilizar el arco de i a j por logistica. \\ 
PS & i, j & Probabilidad de que se pueda utilizar el arco de i a j por seguridad. \\ 
\hline 
\end{tabularx}
\subsection*{Variables}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
Insatis &  & Demanda insatisfecha total en toneladas. \\ 
Eq &  & Maxima proporcion de demanda insatisfecha. \\ 
Seguridad &  & Logaritmo de la probabilidad de que todos los convoys puedan pasar por todos los arcos elegidos por motivos de seguridad. \\ 
Fiabilidad &  & Logaritmo de la probabilidad de que todos los convoys puedan pasar por todos los arcos elegidos por motivos logisticos. \\ 
Coste\_rehab &  & Presupuesto invertido en rehabilitacion en millones de USD. \\ 
Coste &  & Presupuesto invertido en transporte en USD. \\ 
Z & j & Ayuda total en toneladas repartida a j. \\ 
H & i, j & Variable binaria que determina si se habilita el arco de i a j. \\ 
Xmas & i, j, v & Flujo positivo de convoyes de tipo v en el arco de i a j. \\ 
Xmenos & i, j, v & Flujo negativo de convoyes de tipo v en el arco de i a j. \\ 
Y & j, v & Cantidad de convoyes de tipo v que reparten su mercancia en j. \\ 
Usado & i, j & Variable binaria que determina si se usa el arco de i a j. \\ 
Insatisaux & j & Demanda insatisfecha en toneladas en el nodo j. \\ 
Prop\_insatis & j & Proporcion de demanda insatisfecha en el nodo j. \\ 
\hline 
\end{tabularx}
\subsection*{Equations}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
\hyperref[flujo_destinos]{flujo\_destinos}  & j, v & Define el flujo positivo en los nodos con demanda. \\ 
\hyperref[def_Y]{def\_Y}  & j, v & Ecuacion de flujo. \\ 
\hyperref[arcos_buenos_mas]{arcos\_buenos\_mas}  & i, j, v & Consideran de flujo nulo uno de los sentidos de cada arco. \\ 
\hyperref[arcos_buenos_menos]{arcos\_buenos\_menos}  & i, j, v &  \\ 
\hyperref[existencia_ruta_max]{existencia\_ruta\_max}  & i, j, v & Impiden el paso por arcos no existentes. \\ 
\hyperref[existencia_ruta_min]{existencia\_ruta\_min}  & i, j, v &  \\ 
\hyperref[usabilidad_ruta_max]{usabilidad\_ruta\_max}  & i, j, v & Impiden el paso por arcos no usables ni rehabilitados. \\ 
\hyperref[usabilidad_ruta_min]{usabilidad\_ruta\_min}  & i, j, v &  \\ 
\hyperref[lim_demanda]{lim\_demanda}  & j & Impide superar la demanda de cada nodo. \\ 
\hyperref[def_Z]{def\_Z}  & j & Ayuda total en toneladas que llega al nodo j. \\ 
\hyperref[def_insatis]{def\_insatis}  &  & Determina la demanda insatisfecha total. \\ 
\hyperref[def_eq]{def\_eq}  & j & Determina la maxima proporcion de demanda insatisfecha. \\ 
\hyperref[def_prop]{def\_prop}  & j & Determina la proporcion de demanda insatisfecha en el nodo j. \\ 
\hyperref[def_insatisaux]{def\_insatisaux}  & j & Determina la demanda insatisfecha en el nodo j. \\ 
\hyperref[def_coste_rehab]{def\_coste\_rehab}  &  & Determina el coste invertido en la rehabilitacion. \\ 
\hyperref[lim_coste_rehab]{lim\_coste\_rehab}  &  & Establece el limite de presupuesto para el reparto. \\ 
\hyperref[def_coste]{def\_coste}  &  & Determina el coste invertido en el reparto. \\ 
\hyperref[lim_coste]{lim\_coste}  &  & Establece el limite de presupuesto para el reparto. \\ 
\hyperref[lim_ayuda]{lim\_ayuda}  &  & Establece el limite de toneladas de ayuda. \\ 
\hyperref[arco_usado]{arco\_usado}  & i, j & Determina si el arco de i a j se ha utilizado. \\ 
\hyperref[def_seguridad]{def\_seguridad}  &  & Determina el valor de la seguridad. \\ 
\hyperref[lim_seguridad]{lim\_seguridad}  &  & Obliga a la solucion a contemplar una seguridad minima. \\ 
\hyperref[def_fiabilidad]{def\_fiabilidad}  &  & Determina el valor de la fiabilidad. \\ 
\hyperref[lim_fiabilidad]{lim\_fiabilidad}  &  & Obliga a la solucion a contemplar una fiabilidad minima. \\ 
\hline 
\end{tabularx}
\section*{Equation Definitions}
\subsubsection*{$\text{flujo\_destinos}_{j, v}$} \label{flujo_destinos}
$
\textcolor{black}{\text{Y}}_{j, v} \geq 0
$
\hfill
$
\forall{ j, v}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   >  1 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_Y}_{j, v}$} \label{def_Y}
$
\textcolor{black}{\text{Y}}_{j, v} = \displaystyle \sum_{i} ( \textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} )  - \displaystyle \sum_{i} ( \textcolor{black}{\text{Xmas}}_{j, i, v} - \textcolor{black}{\text{Xmenos}}_{j, i, v} ) 
$
\hfill
$
\forall{ j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{arcos\_buenos\_mas}_{i, j, v}$} \label{arcos_buenos_mas}
$
\textcolor{black}{\text{Xmas}}_{j, i, v} = 0
$
\hfill
$
\forall{ i, j, v}  ~ | ~ (  (  \text{ord}( \textcolor{black}{\text{i}} )   \leq   \text{ord}( \textcolor{black}{\text{j}} )  )  \wedge  ( \textcolor{black}{\text{E}}_{i, j}  >  0 )  )
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{arcos\_buenos\_menos}_{i, j, v}$} \label{arcos_buenos_menos}
$
\textcolor{black}{\text{Xmenos}}_{j, i, v} = 0
$
\hfill
$
\forall{ i, j, v}  ~ | ~ (  (  \text{ord}( \textcolor{black}{\text{i}} )   \leq   \text{ord}( \textcolor{black}{\text{j}} )  )  \wedge  ( \textcolor{black}{\text{E}}_{i, j}  >  0 )  )
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{existencia\_ruta\_max}_{i, j, v}$} \label{existencia_ruta_max}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \leq \textcolor{black}{\text{M}} \cdot \textcolor{black}{\text{E}}_{i, j}
$
\hfill
$
\forall{ i, j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{existencia\_ruta\_min}_{i, j, v}$} \label{existencia_ruta_min}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \geq - ( \textcolor{black}{\text{M}} \cdot \textcolor{black}{\text{E}}_{i, j} ) 
$
\hfill
$
\forall{ i, j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{usabilidad\_ruta\_max}_{i, j, v}$} \label{usabilidad_ruta_max}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \leq \textcolor{black}{\text{M}} \cdot  ( \textcolor{black}{\text{U}}_{i, j} + \textcolor{black}{\text{H}}_{i, j} ) 
$
\hfill
$
\forall{ i, j, v}  ~ | ~ ( \textcolor{black}{\text{E}}_{i, j}  >  0 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{usabilidad\_ruta\_min}_{i, j, v}$} \label{usabilidad_ruta_min}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \geq - ( \textcolor{black}{\text{M}} \cdot  ( \textcolor{black}{\text{U}}_{i, j} + \textcolor{black}{\text{H}}_{i, j} )  ) 
$
\hfill
$
\forall{ i, j, v}  ~ | ~ ( \textcolor{black}{\text{E}}_{i, j}  >  0 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_demanda}_{j}$} \label{lim_demanda}
$
\textcolor{black}{\text{Z}}_{j} \leq \textcolor{black}{\text{dem}}_{j}
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_Z}_{j}$} \label{def_Z}
$
\textcolor{black}{\text{Z}}_{j} = \displaystyle \sum_{v} ( \textcolor{black}{\text{Y}}_{j, v} \cdot \textcolor{black}{\text{capacidad}}_{v} ) 
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_insatis}$} \label{def_insatis}
$
\textcolor{black}{\text{Insatis}} = \displaystyle \sum_{j}\textcolor{black}{\text{Insatisaux}}_{j}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_eq}_{j}$} \label{def_eq}
$
\textcolor{black}{\text{Eq}} \geq \textcolor{black}{\text{Prop\_insatis}}_{j}
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_prop}_{j}$} \label{def_prop}
$
\textcolor{black}{\text{Prop\_insatis}}_{j} \cdot \textcolor{black}{\text{dem}}_{j} = \textcolor{black}{\text{Insatisaux}}_{j}
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_insatisaux}_{j}$} \label{def_insatisaux}
$
\textcolor{black}{\text{Insatisaux}}_{j} \geq \textcolor{black}{\text{dem}}_{j} - \textcolor{black}{\text{Z}}_{j}
$
\hfill
$
\forall{ j}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   >  1 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_coste\_rehab}$} \label{def_coste_rehab}
$
\textcolor{black}{\text{Coste\_rehab}} = \displaystyle \sum_{i} ( \displaystyle \sum_{j} ( \textcolor{black}{\text{coste\_rehabilitacion}}_{i, j} \cdot \textcolor{black}{\text{H}}_{i, j} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_coste\_rehab}$} \label{lim_coste_rehab}
$
\textcolor{black}{\text{Coste\_rehab}} \leq \textcolor{black}{\text{max\_coste\_rehab}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_coste}$} \label{def_coste}
$
\textcolor{black}{\text{Coste}} = -\displaystyle \sum_{v} ( \textcolor{black}{\text{Y}}_{\texttt{Allenby\_}, v} \cdot \textcolor{black}{\text{coste\_fijo}}_{v} )  + \displaystyle \sum_{v} ( \displaystyle \sum_{i} ( \displaystyle \sum_{j} (  ( \textcolor{black}{\text{Xmas}}_{i, j, v} + \textcolor{black}{\text{Xmenos}}_{i, j, v} )  \cdot \textcolor{black}{\text{dist}}_{i, j} \cdot \textcolor{black}{\text{coste\_variable}}_{v} )  )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_coste}$} \label{lim_coste}
$
\textcolor{black}{\text{Coste}} \leq \textcolor{black}{\text{max\_coste}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_ayuda}$} \label{lim_ayuda}
$
-\displaystyle \sum_{v} ( \textcolor{black}{\text{Y}}_{\texttt{Allenby\_}, v} \cdot \textcolor{black}{\text{capacidad}}_{v} )  \leq \textcolor{black}{\text{max\_ayuda}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{arco\_usado}_{i, j}$} \label{arco_usado}
$
\textcolor{black}{\text{M}} \cdot \textcolor{black}{\text{Usado}}_{i, j} \geq \displaystyle \sum_{v} ( \textcolor{black}{\text{Xmas}}_{i, j, v} + \textcolor{black}{\text{Xmenos}}_{j, i, v} ) 
$
\hfill
$
\forall{ i, j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_seguridad}$} \label{def_seguridad}
$
\textcolor{black}{\text{Seguridad}} = \displaystyle \sum_{i} ( \displaystyle \sum_{j} ( \textcolor{black}{\text{Usado}}_{i, j} \cdot \textcolor{black}{\text{PS}}_{i, j} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_seguridad}$} \label{lim_seguridad}
$
\textcolor{black}{\text{Seguridad}} \geq \textcolor{black}{\text{PS\_total}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_fiabilidad}$} \label{def_fiabilidad}
$
\textcolor{black}{\text{Fiabilidad}} = \displaystyle \sum_{i} ( \displaystyle \sum_{j} ( \textcolor{black}{\text{Usado}}_{i, j} \cdot \textcolor{black}{\text{P}}_{i, j} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_fiabilidad}$} \label{lim_fiabilidad}
$
\textcolor{black}{\text{Fiabilidad}} \geq \textcolor{black}{\text{P\_total}}
$ \vspace{5pt}
\hrule 
\bigskip
$\text{Xmas}_{i, j, v}\in \mathbb{Z}_{+} \forall i, j, v$ \newline 
$\text{Xmenos}_{i, j, v}\in \mathbb{Z}_{+} \forall i, j, v$ \newline 
$\text{H}_{i, j}\in \{0,1\} ~ \forall i, j$ \newline 
$\text{Insatisaux}_{j}\geq 0 ~ \forall j$ \newline 
$\text{Prop\_insatis}_{j}\geq 0 ~ \forall j$ \newline 
$\text{Usado}_{i, j}\in \{0,1\} ~ \forall i, j$ \newline
\bigskip
\end{document}