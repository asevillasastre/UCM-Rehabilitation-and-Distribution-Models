\section*{Symbols}
\subsection*{Sets}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
j, i & * & nodos. representan los asentamientos. en este modelo hemos de distinguir entre transitorios-destino-origen \\ 
v & * & Tipos de vehiculo \\ 
\hline 
\end{tabularx}
\subsection*{Parameters}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
dem & j & demanda por cada asentamiento de un unico recurso en cierta unidad continua \\ 
max\_coste &  & Maximo presupuesto que se puede invertir \\ 
max\_puentes &  & maximo numero de puentes que podemos reconstruir previo paso a los camiones \\ 
capacidad\_vehiculo & v & Capacidad de hacer partir 1 vehiculo de tipo v \\ 
coste\_vehiculo & v & Coste total y fijo de hacer partir 1 vehiculo de tipo v \\ 
E & i, j & existe la ruta de i a j \\ 
U & i, j & al inicio es utilizable la ruta de i a j \\ 
\hline 
\end{tabularx}
\subsection*{Variables}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
Xmas & i, j, v & el flujo es discreto. esta variable sirve para denotar el flujo positivo \\ 
Xmenos & i, j, v & esta variable sirve para denotar el flujo negativo \\ 
Y & j, v & flujo de vehiculos que se queda en j \\ 
Z\_ & j & toneladas que se quedan en j \\ 
auxZ & j & variable binaria que determina si llega alguna ayuda a j \\ 
H & i, j & variable binaria que determina si habilitamos el puente de i a j \\ 
Insatis &  &  \\ 
Insatisaux &  &  \\ 
Eq &  &  \\ 
\hline 
\end{tabularx}
\subsection*{Equations}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
\hyperref[definir_Y]{definir\_Y}  & j, v & ecuacion de flujo \\ 
\hyperref[solo_arcos_buenos_mas]{solo\_arcos\_buenos\_mas}  & i, j, v & consideramos de flujo nulo la mitad de los nodos. asi consideramos un grafo no orientado \\ 
\hyperref[solo_arcos_buenos_menos]{solo\_arcos\_buenos\_menos}  & i, j, v &  \\ 
\hyperref[lim_provisiones]{lim\_provisiones}  &  & restriccion que fija la cantidad de ayuda que podemos enviar \\ 
\hyperref[flujo_nulo]{flujo\_nulo}  & j, v & en los nodos transitorios el flujo es nulo \\ 
\hyperref[flujo_destinos]{flujo\_destinos}  & j, v &  \\ 
\hyperref[existencia_ruta]{existencia\_ruta}  & i, j, v &  \\ 
\hyperref[usabilidad_ruta]{usabilidad\_ruta}  & i, j, v &  \\ 
\hyperref[existencia_ruta_min]{existencia\_ruta\_min}  & i, j, v & en este modelo son necesarias 2 restricciones para que el flujo sea nulo y no negativo en los arcos no existentes \\ 
\hyperref[usabilidad_ruta_min]{usabilidad\_ruta\_min}  & i, j, v & ídem con los no usables \\ 
\hyperref[lim_puentes]{lim\_puentes}  &  &  \\ 
\hyperref[def_insatisaux]{def\_insatisaux}  & * & definimos la demanda insatisfecha \\ 
\hyperref[def_auxZ1]{def\_auxZ1}  & j & definimos los nodos a los que llega alguna ayuda \\ 
\hyperref[def_auxZ2]{def\_auxZ2}  & j & a los nodos transitorios no puede llegar ninguna ayuda por lo que no cuentan \\ 
\hyperref[def_Z_]{def\_Z\_}  & j &  \\ 
\hyperref[f_insatis]{f\_insatis}  &  & funcion objetivo 1. demanda insatisfecha total \\ 
\hyperref[f_eq]{f\_eq}  &  & funcion objetivo 2. criterio de equidad. numero de nodos a los que se les reparte alguna cantidad de ayuda \\ 
\hline 
\end{tabularx}
\section*{Equation Definitions}
\subsubsection*{$\text{definir\_Y}_{j, v}$} \label{definir_Y}
$
\textcolor{black}{\text{Y}}_{j, v} = \displaystyle \sum_{i} ( \textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} )  - \displaystyle \sum_{i} ( \textcolor{black}{\text{Xmas}}_{j, i, v} - \textcolor{black}{\text{Xmenos}}_{j, i, v} ) 
$
\hfill
$
\forall{ j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{solo\_arcos\_buenos\_mas}_{i, j, v}$} \label{solo_arcos_buenos_mas}
$
\textcolor{black}{\text{Xmas}}_{j, i, v} = 0
$
\hfill
$
\forall{ i, j, v}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{i}} )   \leq   \text{ord}( \textcolor{black}{\text{j}} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{solo\_arcos\_buenos\_menos}_{i, j, v}$} \label{solo_arcos_buenos_menos}
$
\textcolor{black}{\text{Xmenos}}_{j, i, v} = 0
$
\hfill
$
\forall{ i, j, v}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{i}} )   \leq   \text{ord}( \textcolor{black}{\text{j}} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_provisiones}$} \label{lim_provisiones}
$
\displaystyle \sum_{v} ( \textcolor{black}{\text{Y}}_{\texttt{0}, v} \cdot \textcolor{black}{\text{coste\_vehiculo}}_{v} )  \geq -\textcolor{black}{\text{max\_coste}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{flujo\_nulo}_{j, v}$} \label{flujo_nulo}
$
\textcolor{black}{\text{Y}}_{j, v} = 0
$
\hfill
$
\forall{ j, v}  ~ | ~ (  (  \text{ord}( \textcolor{black}{\text{j}} )   >  1 )  \wedge  (  \text{ord}( \textcolor{black}{\text{j}} )   \leq  4 )  )
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{flujo\_destinos}_{j, v}$} \label{flujo_destinos}
$
\textcolor{black}{\text{Y}}_{j, v} \geq 0
$
\hfill
$
\forall{ j, v}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   \geq  5 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{existencia\_ruta}_{i, j, v}$} \label{existencia_ruta}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \leq 99 \cdot \textcolor{black}{\text{E}}_{i, j}
$
\hfill
$
\forall{ i, j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{usabilidad\_ruta}_{i, j, v}$} \label{usabilidad_ruta}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \leq 99 \cdot  ( \textcolor{black}{\text{U}}_{i, j} + \textcolor{black}{\text{H}}_{i, j} ) 
$
\hfill
$
\forall{ i, j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{existencia\_ruta\_min}_{i, j, v}$} \label{existencia_ruta_min}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \geq - ( 99 \cdot \textcolor{black}{\text{E}}_{i, j} ) 
$
\hfill
$
\forall{ i, j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{usabilidad\_ruta\_min}_{i, j, v}$} \label{usabilidad_ruta_min}
$
\textcolor{black}{\text{Xmas}}_{i, j, v} - \textcolor{black}{\text{Xmenos}}_{i, j, v} \geq - ( 99 \cdot  ( \textcolor{black}{\text{U}}_{i, j} + \textcolor{black}{\text{H}}_{i, j} )  ) 
$
\hfill
$
\forall{ i, j, v} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_puentes}$} \label{lim_puentes}
$
\displaystyle \sum_{i} ( \displaystyle \sum_{j}\textcolor{black}{\text{H}}_{i, j} )  \leq \textcolor{black}{\text{max\_puentes}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_insatisaux}_{j}$} \label{def_insatisaux}
$
\textcolor{black}{\text{Insatisaux}} \geq \textcolor{black}{\text{dem}}_{j} - \textcolor{black}{\text{Z\_}}_{j}
$
\hfill
$
\forall{ j}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   \geq  5 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_auxZ1}_{j}$} \label{def_auxZ1}
$
\textcolor{black}{\text{auxZ}}_{j} \leq 99 \cdot \textcolor{black}{\text{Z\_}}_{j}
$
\hfill
$
\forall{ j}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   \geq  5 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_auxZ2}_{j}$} \label{def_auxZ2}
$
\textcolor{black}{\text{auxZ}}_{j} = 0
$
\hfill
$
\forall{ j}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   \leq  4 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_Z\_}_{j}$} \label{def_Z_}
$
\textcolor{black}{\text{Z\_}}_{j} = \displaystyle \sum_{v} ( \textcolor{black}{\text{Y}}_{j, v} \cdot \textcolor{black}{\text{capacidad\_vehiculo}}_{v} ) 
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{f\_eq}$} \label{f_eq}
$
\textcolor{black}{\text{Eq}} = \displaystyle \sum_{j}\textcolor{black}{\text{auxZ}}_{j}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{f\_insatis}$} \label{f_insatis}
$
\textcolor{black}{\text{Insatis}} = \displaystyle \sum_{j}\textcolor{black}{\text{Insatisaux}}
$ \vspace{5pt}
\hrule 
\bigskip
$\text{Xmas}_{i, j, v}\in \mathbb{Z}_{+} \forall i, j, v$ \newline 
$\text{Xmenos}_{i, j, v}\in \mathbb{Z}_{+} \forall i, j, v$ \newline 
$\text{H}_{i, j}\in \{0,1\} ~ \forall i, j$ \newline 
$\text{Insatisaux}\geq 0 ~ \forall $ \newline 
$\text{auxZ}_{j}\in \{0,1\} ~ \forall j$ \newline
\bigskip
\end{document}