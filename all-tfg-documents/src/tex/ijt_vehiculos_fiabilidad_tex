\usepackage[american]{babel}
\usepackage{amsmath}
\usepackage{amssymb}
\usepackage[hidelinks]{hyperref}
\usepackage{tabularx}
\usepackage{ltablex}
\keepXColumns
\usepackage{xcolor}
