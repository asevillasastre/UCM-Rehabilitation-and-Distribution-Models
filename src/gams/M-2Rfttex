\begin{document}
\section*{Symbols}
\subsection*{Sets}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
j, i & * & Nodos. \\ 
v & * & Tipos de vehiculo. \\ 
w & * & Vehiculos posibles de tipo v. \\ 
\hline 
\end{tabularx}
\subsection*{Parameters}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
dem & j & Demanda de cada nodo en toneladas de paquetes de ayuda. \\ 
capacidad\_vehiculo & v & Capacidad en toneladas de una unidad vehicular de tipo v. \\ 
coste\_fijo & v & Coste fijo en euros de hacer partir un vehiculo de tipo v. \\ 
coste\_variable &  & Coste variable en euros por km recorrido por cualquier vehiculo. \\ 
max\_coste &  & Maximo presupuesto que se puede invertir en total en la mision. \\ 
max\_habilitaciones &  & Maximo numero de arcos que se pueden rehabilitar previo paso a los vehiculos. \\ 
P\_total &  & Probabilidad aceptable de que todos los convoys puedan pasar por todas las rutas elegidas. \\ 
T\_max &  & Tiempo maximo asumible en horas para el reparto de toda la ayuda. \\ 
velocidad\_vehiculo & v & Inversa de la velocidad en h por km que recorre un vehiculo de tipo v. \\ 
E & i, j & Existe el arco de i a j. \\ 
U & i, j & El inicio es utilizable el arco de i a j. \\ 
dist & i, j & Distancia de las rutas de i a j en km. \\ 
coste\_rehabilitacion & i, j & Coste de rehabilitar el arco de i a j. \\ 
P & i, j & Probabilidad de que se pueda utilizar el arco de i a j. \\ 
\hline 
\end{tabularx}
\subsection*{Variables}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
X & i, j, v, w & El vehiculo de tipo v w-esimo va de i a j. \\ 
Z & j, v, w & El vehiculo de tipo v w-esimo reparte a j. \\ 
Ayuda & j & Ayuda total repartida a j. \\ 
H & i, j & Habilitamos el arco de i a j. \\ 
Insatis &  & Total demanda insatisfecha. \\ 
Coste &  & Coste total del proyecto. \\ 
Fiabilidad &  & Logaritmo de la robabilidad de que todos los convoys puedan pasar por todos los arcos elegidos. \\ 
Usado & i, j & Variable binaria que determina si se usa el arco de i a j. \\ 
t & j, v, w & Instante de tiempo en el que el vehículo w-esimo de tipo v llega a j. \\ 
\hline 
\end{tabularx}
\subsection*{Equations}
\begin{tabularx}{\textwidth}{| l | l | X |} 
\hline 
\textbf{Name} & \textbf{Domains} & \textbf{Description}\\ 
\hline 
\endhead 
\hyperref[si_sales_entraste]{si\_sales\_entraste}  & j, v, w & Un vehiculo solo puede salir de un nodo al que ha entrado. \\ 
\hyperref[Z_entras]{Z\_entras}  & j, v, w & Se reparte en un nodo al que se llega. \\ 
\hyperref[Z_no_sales]{Z\_no\_sales}  & j, v, w & Del nodo al que se reparte no se sale. \\ 
\hyperref[auto_cero]{auto\_cero}  & i, j, v, w & No se producen viajes de un nodo al mismo. \\ 
\hyperref[existencia]{existencia}  & i, j, v, w & Impide el paso por arcos no existentes. \\ 
\hyperref[usabilidad]{usabilidad}  & i, j, v, w & Impide el paso por arcos no usables ni rehabilitados. \\ 
\hyperref[lim_coste]{lim\_coste}  &  &  \\ 
\hyperref[lim_habilitaciones]{lim\_habilitaciones}  &  &  \\ 
\hyperref[def_Ayuda]{def\_Ayuda}  & j & Ayuda total en toneladas que llega al nodo j. \\ 
\hyperref[lim_demanda]{lim\_demanda}  & j & Impide superar la demanda de cada nodo. \\ 
\hyperref[f_Insatis]{f\_Insatis}  &  &  \\ 
\hyperref[def_Coste]{def\_Coste}  &  & Definicion del coste. \\ 
\hyperref[def_fiabilidad]{def\_fiabilidad}  &  & Define el valor de la fiabilidad. \\ 
\hyperref[lim_fiabilidad]{lim\_fiabilidad}  &  & Obliga a la solucion a contemplar una fiabilidad minima. \\ 
\hyperref[arco_usado]{arco\_usado}  & i, j & Determina si el arco de i a j se ha utilizado. \\ 
\hyperref[def_tiempos]{def\_tiempos}  & i, j, v, w & Define los tiempos de llegada de los vehiculos a los nodos. \\ 
\hyperref[lim_tiempo]{lim\_tiempo}  & j, v, w & Establece el limite temporal de la mision. \\ 
\hline 
\end{tabularx}
\section*{Equation Definitions}
\subsubsection*{$\text{si\_sales\_entraste}_{j, v, w}$} \label{si_sales_entraste}
$
\displaystyle \sum_{i}\textcolor{black}{\text{X}}_{i, j, v, w} \geq \displaystyle \sum_{i}\textcolor{black}{\text{X}}_{j, i, v, w}
$
\hfill
$
\forall{ j, v, w}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   >  1 ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{Z\_entras}_{j, v, w}$} \label{Z_entras}
$
\textcolor{black}{\text{Z}}_{j, v, w} \leq \displaystyle \sum_{i}\textcolor{black}{\text{X}}_{i, j, v, w}
$
\hfill
$
\forall{ j, v, w} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{Z\_no\_sales}_{j, v, w}$} \label{Z_no_sales}
$
\textcolor{black}{\text{Z}}_{j, v, w} \leq 1 - \displaystyle \sum_{i}\textcolor{black}{\text{X}}_{j, i, v, w}
$
\hfill
$
\forall{ j, v, w} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{auto\_cero}_{i, j, v, w}$} \label{auto_cero}
$
\textcolor{black}{\text{X}}_{i, j, v, w} = 0
$
\hfill
$
\forall{ i, j, v, w}  ~ | ~ (  \text{ord}( \textcolor{black}{\text{j}} )   =   \text{ord}( \textcolor{black}{\text{i}} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{existencia}_{i, j, v, w}$} \label{existencia}
$
\textcolor{black}{\text{X}}_{i, j, v, w} \leq \textcolor{black}{\text{E}}_{i, j}
$
\hfill
$
\forall{ i, j, v, w} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{usabilidad}_{i, j, v, w}$} \label{usabilidad}
$
\textcolor{black}{\text{X}}_{i, j, v, w} \leq \textcolor{black}{\text{U}}_{i, j} + \textcolor{black}{\text{H}}_{i, j}
$
\hfill
$
\forall{ i, j, v, w} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_coste}$} \label{lim_coste}
$
\textcolor{black}{\text{Coste}} \leq \textcolor{black}{\text{max\_coste}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_habilitaciones}$} \label{lim_habilitaciones}
$
\displaystyle \sum_{i} ( \displaystyle \sum_{j}\textcolor{black}{\text{H}}_{i, j} )  \leq \textcolor{black}{\text{max\_habilitaciones}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_Ayuda}_{j}$} \label{def_Ayuda}
$
\textcolor{black}{\text{Ayuda}}_{j} = \displaystyle \sum_{v} ( \displaystyle \sum_{w} ( \textcolor{black}{\text{Z}}_{j, v, w} \cdot \textcolor{black}{\text{capacidad\_vehiculo}}_{v} )  ) 
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_demanda}_{j}$} \label{lim_demanda}
$
\textcolor{black}{\text{Ayuda}}_{j} \leq \textcolor{black}{\text{dem}}_{j}
$
\hfill
$
\forall{ j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{f\_Insatis}$} \label{f_Insatis}
$
\textcolor{black}{\text{Insatis}} = \displaystyle \sum_{j} ( \textcolor{black}{\text{dem}}_{j} - \textcolor{black}{\text{Ayuda}}_{j} ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_Coste}$} \label{def_Coste}
$
\textcolor{black}{\text{Coste}} = \displaystyle \sum_{i} ( \displaystyle \sum_{j} ( \displaystyle \sum_{v} ( \displaystyle \sum_{w} ( \textcolor{black}{\text{X}}_{i, j, v, w} \cdot \textcolor{black}{\text{coste\_variable}} \cdot \textcolor{black}{\text{dist}}_{i, j} )  )  )  )  + \displaystyle \sum_{i} ( \displaystyle \sum_{j} ( \textcolor{black}{\text{H}}_{i, j} \cdot \textcolor{black}{\text{coste\_rehabilitacion}}_{i, j} )  )  + \displaystyle \sum_{j} ( \displaystyle \sum_{v} ( \displaystyle \sum_{w} ( \textcolor{black}{\text{X}}_{\texttt{0}, j, v, w} \cdot \textcolor{black}{\text{coste\_fijo}}_{v} )  )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_fiabilidad}$} \label{def_fiabilidad}
$
\textcolor{black}{\text{Fiabilidad}} = \displaystyle \sum_{i} ( \displaystyle \sum_{j} ( \textcolor{black}{\text{Usado}}_{i, j} \cdot \textcolor{black}{\text{P}}_{i, j} )  ) 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_fiabilidad}$} \label{lim_fiabilidad}
$
\textcolor{black}{\text{Fiabilidad}} \geq \textcolor{black}{\text{P\_total}}
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{arco\_usado}_{i, j}$} \label{arco_usado}
$
\textcolor{black}{\text{Usado}}_{i, j} \geq \displaystyle \sum_{v} ( \displaystyle \sum_{w}\textcolor{black}{\text{X}}_{i, j, v, w} ) 
$
\hfill
$
\forall{ i, j} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{def\_tiempos}_{i, j, v, w}$} \label{def_tiempos}
$
\textcolor{black}{\text{t}}_{j, v, w} - \textcolor{black}{\text{t}}_{i, v, w} + \textcolor{black}{\text{dist}}_{i, j} \cdot \textcolor{black}{\text{velocidad\_vehiculo}}_{v} \leq \textcolor{black}{\text{T\_max}} \cdot  ( 1 - \textcolor{black}{\text{X}}_{i, j, v, w} ) 
$
\hfill
$
\forall{ i, j, v, w} 
$ \vspace{5pt}
\hrule 
\subsubsection*{$\text{lim\_tiempo}_{j, v, w}$} \label{lim_tiempo}
$
\textcolor{black}{\text{t}}_{j, v, w} \leq \textcolor{black}{\text{T\_max}}
$
\hfill
$
\forall{ j, v, w} 
$ \vspace{5pt}
\hrule 
\bigskip
$\text{X}_{i, j, v, w}\in \{0,1\} ~ \forall i, j, v, w$ \newline 
$\text{Z}_{j, v, w}\in \{0,1\} ~ \forall j, v, w$ \newline 
$\text{H}_{i, j}\in \{0,1\} ~ \forall i, j$ \newline 
$\text{Ayuda}_{j}\in \mathbb{Z}_{+} \forall j$ \newline 
$\text{Usado}_{i, j}\in \{0,1\} ~ \forall i, j$ \newline
\bigskip
\end{document}